                    <td><%=data.isverifed %></td>
                    <td><%=data.dname %></td>
                    <td><%=data.passingyear %></td>
                    <td><%=data.result10 %></td>
                    <td><%=data.result12 %></td>
                    <td><%=data.avgcgpa %></td>
                    <td><%=data.backlogs %></td>
                </tr>
                <% } i++; }) %>
                <% } else{ %>
                    <tr>
                        <td colspan="7">No Data Found</td>
                    </tr>
                <% } %>
               </tbody>
