<%- include ('../partials/header.html'); -%>
		<title>Add Students</title>
		<link rel="stylesheet" href="../../css/tpo.css">
</head>
<body>
    <%- include ('partials/navbar.html'); -%>
    
    <br/><br/><br/><br/>
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-3"></div>
            <div class="col-md-6">
                <hr class="hl1">
                <div class = "page-header bg-light">
                    <h1 class="text-center">
                        Add Students
                    </h1>
                 </div>
                 <hr class="hl1"><br/>
            </div>
        </div>
       
    <div class ="row">
    <div class="col-md-4"></div>
    <div class="col-md-4 max-auto form-group bg-light border rounded border-dark">
        <form class="form-horizontal" role="form" method="post" action="/tpo/addstudent" enctype="multipart/form-data">
            <div class="form-group">
                <br/>
               <label class="col-md-6 control-label">Add ExcelFile here!!</label><hr>
               <div class="col-sm-12">
                   <input class="form-control" type="file" name="uploadfile" accept=""/>
               </div>
           </div>    <hr><hr>
           <% if (locals.message) { %>
              <div class="alert alert-<%= status %> col-sm-12"><%= message %></div>
          <% } %>                            
           <div class="form-group">
               <!-- Button -->                                       
               <div class="col-md-offset-3 col-md-9">
                   <button id="btn-signup" type="submit" class="btn btn-outline-info"><i class="icon-hand-right"></i> Upload</button>
               </div>
           </div>  
       </form>
    </div>
    <div class="col-md-4"></div>
    </div>
</div>
<%- include ('../partials/footer.html'); -%>